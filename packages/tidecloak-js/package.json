{
  "name": "tidecloak-js",
  "version": "0.0.1",
  "type": "module",
  "description": "TideCloak client side JS SDK",
  "exports": {
    ".": {
      "types": "./dist/lib/heimdall.d.ts",
      "default": "./dist/lib/heimdall.js"
    },
    "./IAMService": {
      "types": "./dist/src/IAMService.d.ts",
      "default": "./dist/src/IAMService.js"
    }
  },
  "files": [
    "dist",
    "scripts/postinstall.cjs"
  ],
  "scripts": {
    "postinstall": "node ./scripts/postinstall.cjs",
    "build": "tsc && cp ./silent-check-sso.html dist/",
    "prepare": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tide-foundation/tidecloak-js.git"
  },
  "author": "Tide",
  "homepage": "https://tide.org",
  "license": "https://tide.org/licenses_tcoc2-0-0-en",
  "keywords": [
    "security",
    "tidecloak",
    "tide",
    "authentication",
    "oidc",
    "oauth2",
    "authorization"
  ]
}
