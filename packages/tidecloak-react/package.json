{
  "name": "@tidecloak/react",
  "version": "0.0.1",
  "description": "TideCloak client-side React SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tide-foundation/tidecloak-js.git"
  },
  "author": "Tide",
  "license": "https://tide.org/licenses_tcoc2-0-0-en",
  "keywords": [
    "security",
    "tidecloak",
    "tide",
    "authentication",
    "oidc",
    "oauth2",
    "authorization",
    "react"
  ],
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6"
  },
  "dependencies": {
    "@tidecloak/js": "file:../tidecloak-js/tidecloak-js-0.0.1.tgz"
  }
}
