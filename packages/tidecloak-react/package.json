{
  "name": "@tidecloak/react",
  "version": "0.0.1",
  "type": "module",
  "description": "TideCloak client-side React SDK",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "scripts/postinstall.cjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tide-foundation/tidecloak-js.git"
  },
  "author": "Tide",
  "license": "https://tide.org/licenses_tcoc2-0-0-en",
  "keywords": [
    "security",
    "tidecloak",
    "tide",
    "authentication",
    "oidc",
    "oauth2",
    "authorization",
    "react"
  ],
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "tidecloak-js": "file:../tidecloak-js/tidecloak-js-0.0.1.tgz"
  },
  "scripts": {
    "build": "tsc && cp ./silent-check-sso.html dist/",
    "prepare": "npm run build",
    "postinstall": "node ./scripts/postinstall.cjs"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6"
  }
}
