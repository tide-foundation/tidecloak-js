{
  "name": "@tidecloak/nextjs",
  "version": "0.0.1",
  "type": "module",
  "description": "TideCloak client-side React SDK",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts",
      "require": "./dist/server/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tide-foundation/tidecloak-js.git"
  },
  "author": "Tide",
  "license": "https://tide.org/licenses_tcoc2-0-0-en",
  "keywords": [
    "security",
    "tidecloak",
    "tide",
    "authentication",
    "oidc",
    "oauth2",
    "authorization",
    "react"
  ],
  "peerDependencies": {
    "next": ">=13",
    "react": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@tidecloak/react": "file:../tidecloak-react/tidecloak-react-0.0.1.tgz",
    "tide-jwt": "file:../TideJWT/tide-jwt-0.0.1.tgz",
    "tidecloak-js": "file:../tidecloak-js/tidecloak-js-0.0.1.tgz"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
