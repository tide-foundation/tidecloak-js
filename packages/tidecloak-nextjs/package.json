{
  "name": "@tidecloak/nextjs",
  "version": "0.12.15",
  "description": "TideCloak nextjs SDK",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./server": {
      "types": "./dist/types/server/index.d.ts",
      "edge-light": "./dist/esm/server/index.js",
      "import": "./dist/esm/server/index.js",
      "require": "./dist/cjs/server/index.js",
      "default": "./dist/esm/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tide-foundation/tidecloak-js.git"
  },
  "author": "Tide",
  "license": "https://tide.org/licenses_tcoc2-0-0-en",
  "keywords": [
    "security",
    "tidecloak",
    "tide",
    "authentication",
    "oidc",
    "oauth2",
    "authorization",
    "react",
    "nextjs"
  ],
  "peerDependencies": {
    "next": "^16.0.1",
    "react": "^19.2.0"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@tidecloak/react": "^0.13.11",
    "@tidecloak/verify": "^0.13.11"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "next": "^15.0.0"
  }
}
